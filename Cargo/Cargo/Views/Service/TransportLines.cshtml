@{
    ViewBag.Title = "TransportLines";
}
@model Cargo.App_DataClass.TransportLines
<div class="clear-fix">
</div>
<div class="secondrow" style="padding: 20px; padding-left: 100px;">
    <table width="100%" border="0" cellspacing="10" cellpadding="0">
        <tbody>
            <tr>
                <td height="200" align="left" valign="top" class="font12bai">
                    @foreach (var item in Model.Lines)
                    {
                        @item.LineText <br />
                    }
                </td>
            </tr>
        </tbody>
    </table>
</div>
