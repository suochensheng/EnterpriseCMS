                     <br />
                    <p>@Model.BlockText_2</p>

                </div>
           
            </div>

            <div class="item half odd">
                <img alt="@Model.BlockTitle_3" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_3)"/>
                <div class="description hided">
                    <time>@Model.BlockTime_3</time>
                    <h4>@Model.BlockTitle_3</h4>

                    <p>@Model.BlockText_3 </p>

                </div>
        
            </div>

        </div>
        <div class="gr-box">
            <div class="item half even">
                <img alt="@Model.BlockTitle_4" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_4)"/>
                <div class="description hided">
                    <time>@Model.BlockTime_4</time>
                    <h4>@Model.BlockTitle_4</h4>

                    <p>@Model.BlockText_4</p>

                </div>
        
            </div>

            <div class="item half odd">
                <img alt="@Model.BlockTitle_5" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_5)"/>
                <div class="description hided">
                    <time>@Model.BlockTime_5</time>
                    <h4>@Model.BlockTitle_5</h4>

                    <p>@Model.BlockText_5</p>

                </div>
        
            </div>

            <div class="item large 6 2 even">
                <img alt="@Model.BlockTitle_6" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_6)"/>
                <div class="description hided">
                    <time>@Model.BlockTime_6</time>
                    <h4>@Model.BlockTitle_6</h4>

                    <p>@Model.BlockText_6</p>

                </div>
 
            </div>

        </div>
    </div>

</div>