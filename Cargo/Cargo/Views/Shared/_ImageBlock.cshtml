@model Cargo.App_DataClass.Strength

<div class="wrap" style="padding-left:30px; padding-right:10px;">

    <div class="grid">
        <div class="gr-box">
            <div class="item large 1 1 odd">
                <img alt="@Model.BlockTitle_1" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_1)" />
                <div class="description disp">
                    <time>@Model.BlockTime_1</time>
                    <h4>@Model.BlockTitle_1</h4>
                    <br />
                    <p>@Model.BlockText_1 </p>

                </div>
      
            </div>

            <div class="item half even">
                <img alt="@Model.BlockTitle_2" style="margin: 0 0;" src="@Url.Content(Model.BlockImage_2)"/>
